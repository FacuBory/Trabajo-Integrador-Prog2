<!DOCTYPE html>
<html lang="es">
	

	<%- include('./partials/head') %> 
<body>
	<%- include('./partials/header-deslogueado') %> 
	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for=""> Email </label>
					<input type="email" email="email">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for=""> Usuario </label>
					<input type="text" name="usuario">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Contraseña </label>
					<input type="password" name="contraseña">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Fecha de nacimiento </label>
					<input type="date" name="nacimiento">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">nro de documento </label>
					<input type="number" name="nro de documento">
				</form>
			</h2>
			

	<!-- footer -->
	<%- include('./partials/footer') %> 
	<!-- /footer -->
</body>

</html>