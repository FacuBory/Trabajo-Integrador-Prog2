<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<%- include('./partials/head') %> 
<body>
	<!-- Header -->
<%- include('./partials/header-logueado') %> 
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for=""> Email </label>
					<input type="email" email="email" placeholder="...@gmail.com">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for=""> Usuario </label>
					<input type="text" name="usuario">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Contraseña </label>
					<input type="password" name="contraseña">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Fecha de nacimiento </label>
					<input type="date" name="nacimiento">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Número de documento </label>
					<input type="number" name="numero de documento" maxlength=8 >
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Foto de perfil </label>
					<input type="file" name="foto de perfil">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="/users/profile" action="/users/profile">
					<input type="reset" value="Borrar"></input>
					<input type="submit" value="Enviar"></input>
				</form>
			</h2> 
		</div>
	</div>


	<!-- footer -->
	<%- include('./partials/footer') %> 
</body>

</html>